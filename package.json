{
  "name": "neurofocus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5194",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run tests/integration/**/*.test.{ts,tsx}",
    "test:component": "vitest run src/components/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:a11y": "playwright test --project=a11y-chrome",
    "test:accessibility": "npm run test:a11y",
    "test:performance": "playwright test --project=performance",
    "test:lighthouse": "lighthouse http://localhost:5194 --output html --output-path ./test-results/lighthouse.html",
    "test:security": "npm audit && npm run test:security:custom",
    "test:security:custom": "echo 'Running custom security tests...' && vitest run tests/security/**/*.test.{ts,tsx}",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:coverage && npm run test:e2e",
    "test:full": "npm run test:all && npm run test:a11y && npm run test:performance",
    "test:setup": "playwright install",
    "test:clean": "rm -rf coverage test-results",
    "test:report": "echo 'Opening test reports...' && open coverage/index.html && open test-results/index.html",
    "build:analyze": "vite build --mode production && npx vite-bundle-visualizer",
    "build:prod": "NODE_ENV=production vite build",
    "build:report": "npm run build:prod && npm run analyze",
    "analyze": "npx vite-bundle-visualizer",
    "optimize:images": "npx imagemin src/assets/* --out-dir=src/assets/optimized",
    "lighthouse": "npx lighthouse http://localhost:5194 --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.5",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^3.6.0",
    "recharts": "^2.12.7",
    "zustand": "^4.5.2",
    "firebase": "^10.12.2",
    "tailwind-merge": "^3.3.1",
    "@google/generative-ai": "^0.15.0",
    "web-vitals": "^3.5.0",
    "@radix-ui/react-slot": "^1.1.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "jsdom": "^24.0.0",
    "happy-dom": "^15.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@playwright/test": "^1.47.0",
    "axe-core": "^4.10.0",
    "@axe-core/react": "^4.10.0",
    "lighthouse": "^12.0.0",
    "msw": "^2.4.0",
    "c8": "^10.1.0",
    "@types/jest": "^29.5.0",
    "@tanstack/react-query": "^5.51.0"
  }
}